{
  "urn": "urn:mediator:skeleton-production-mediator",
  "version": "0.0.1",
  "name": "Skeleton Production Bootstrap Mediator",
  "description": "Skeleton Production Bootstrap Mediator",
  "defaultChannelConfig": [
    {
      "name": "Production Route",
      "urlPattern": "^/testEndpoint",
      "routes": [
        {
          "name": "Test Route",
          "host": "mediator",
          "port": "4321",
          "primary": true,
          "type": "http"
        }
      ],
      "allow": [
        "admin"
      ],
      "methods": [
        "GET",
        "POST"
      ],
      "type": "http"
    }
  ],
  "endpoints": [
    {
      "name": "Test Mediator",
      "host": "dhis2server",
      "path": "/testEndpoint",
      "port": "8085",
      "primary": true,
      "type": "http"
    }
  ],
  "configDefs": [
    {
      "param": "dhis",
      "displayName": "DHIS2 Instance Details",
      "description": "The custom DHIS2 instance details for data fetch",
      "type": "struct",
      "array": false,
      "template": [
        {
          "param": "url",
          "displayName": "DHIS2 URL",
          "description": "The DHIS2 instance instance including credentials. ie: https://username:password@yourDhis2Instance.org",
          "type": "string"
        },
        {
          "param": "path",
          "displayName": "DHIS2 Path",
          "description": "The Path to the specific data you want from DHIS2",
          "type": "string"
        }
      ]
    }
  ]
}
